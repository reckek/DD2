mixin card(cardId, type, start, end, progress)
  .status-card.border-clip-2
    .status-card__info.border-clip-2
      .status-card__info-top
        .status-card__id= cardId
        .status-card__type Тип:#{" "}
          span= type
      .status-card__mmr
        .stat
          p= start
          span Стартовый мир
        svg.status-card__arrow(width="59" height="26" viewBox="0 0 59 26" fill="none")
          path(d="M1 13H57M57 13L44.9091 25M57 13L44.9091 1" stroke-width="2" stroke-linecap="round")
        .stat.status-card__mmr-end
          p= end
          span ИТОГОВЫЙ ММР
      labe.status-card__progress
        | {{progress}}%
        progress(value="0")
    .status-card__status
      .status-card__status-header
        svg(width="20" height="20" viewBox="0 0 20 20" fill="none")
          path(d="M8 14L12 10L8 6" stroke-width="2" stroke-linecap="round" stroke-linejoin="round")
        p Выполняется
